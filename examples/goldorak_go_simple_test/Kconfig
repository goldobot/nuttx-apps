config EXAMPLES_GOLDORAK_GO_SIMPLE_TEST
	bool "Goldorak motor control simple example"
	default n
	depends on PWM && NSH_BUILTIN_APPS
	---help---
		Enable the Goldorak motor control simple example (with real-time thread)

if EXAMPLES_GOLDORAK_GO_SIMPLE_TEST

config EXAMPLES_GOLDORAK_GO_SIMPLE_TEST_PWM_FREQUENCY
	int "Default PWM frequency"
	default 10000
	---help---
		The default PWM frequency.  Default: 10000 Hz


endif
